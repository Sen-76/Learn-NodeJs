{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "../",
    "baseUrl": "./",
    "paths": {
      "@common/*": ["../common/*"],
      "@/*": ["*/src"]
    }
  },
  "include": ["src/**/*.ts", "common/models/**/*"],
  "exclude": ["node_modules"]
}
